{
  "name": "In-Browser Test Automator",
  "version": "1.0.0",
  "description": "An in-browser test automator.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "blue_play_button.png"
  },
  "icons": {
    "128": "blue_play_button.png"
  },
  "manifest_version": 2
}
